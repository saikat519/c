#include<stdio.h>
int main()
{
	int first,k=5,flag=0,last,mid,a[]={1,2,3,4,5,6,7,8,9};
	
	first=0;
	last=9;
	while(first<=last)
	{
		mid=(first+last)/2;
		if(a[mid]<k)
		first=mid+1;
		else if(a[mid]>k)
		last=mid-1;
		else if(a[mid]==k)
		{
			printf("%d is found at %d",k,mid);
			flag++;
			break;
		}	
		
	}
	if(flag==0)
	printf("%d not found",k);
return 0;
}
